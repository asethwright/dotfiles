[
  {
    "key": "space p",
    "command": "workbench.action.quickOpen",
    "when": "!inputFocus"
  },
  {
    "key": "space d",
    "command": "projectManager.listProjects",
    "when": "!inputFocus"
  },
  {
    "key": "space space",
    "command": "workbench.action.quickOpen",
    "when": "!inputFocus"
  },
  {
    "key": "space g",
    "command": "workbench.view.scm",
    "when": "workbench.scm.active && !gitlens:disabled && config.gitlens.keymap == 'chorded' && !inputFocus"
  },
  {
    "key": "space /",
    "command": "workbench.action.findInFiles",
    "when": "!inputFocus"
  },
  {
    "key": "space e",
    "command": "workbench.view.explorer",
    "when": "!inputFocus"
  },
  {
    "key": "space '",
    "command": "workbench.action.terminal.toggleTerminal",
    "when": "!inputFocus"
  },
  {
    "key": "space a",
    "command": "workbench.action.toggleActivityBarVisibility",
    "when": "!inputFocus"
  },
  {
    "key": "space b",
    "command": "workbench.action.toggleSidebarVisibility",
    "when": "!inputFocus"
  },
  {
    "key": "space v",
    "command": "workbench.action.toggleStatusbarVisibility",
    "when": "!inputFocus"
  },
  {
    "key": "space '",
    "command": "workbench.action.terminal.toggleTerminal",
    "when": "!inputFocus"
  },
  {
    "key": "ctrl+'",
    "command": "workbench.action.terminal.toggleTerminal"
  },
  {
    "key": "alt+l",
    "command": "workbench.action.terminal.focusNext"
  },
  {
    "key": "alt+h",
    "command": "workbench.action.terminal.focusPrevious"
  },
  {
    "key": "ctrl+j",
    "command": "workbench.action.quickOpenNavigateNext"
  },
  {
    "key": "ctrl+k",
    "command": "workbench.action.quickOpenNavigatePrevious"
  },
  {
    "key": "ctrl+l",
    "command": "extension.vim_navigateRight",
    "when": "vim.active && vim.use<C-w> && !editorTextFocus"
  },
  {
    "key": "ctrl+w l",
    "command": "-extension.vim_navigateRight",
    "when": "vim.active && vim.use<C-w> && !editorTextFocus"
  },
  {
    "key": "ctrl+h",
    "command": "extension.vim_navigateLeft",
    "when": "vim.active && vim.use<C-w> && !editorTextFocus"
  },
  {
    "key": "ctrl+w h",
    "command": "-extension.vim_navigateLeft",
    "when": "vim.active && vim.use<C-w> && !editorTextFocus"
  },
  {
    "key": "shift+cmd+o",
    "command": "-workbench.action.gotoSymbol"
  },
  {
    "key": "ctrl+j",
    "command": "editor.action.moveLinesDownAction",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+cmd+down",
    "command": "-editor.action.moveLinesDownAction",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+j",
    "command": "editor.action.moveLinesDownAction",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "alt+down",
    "command": "-editor.action.moveLinesDownAction",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+k",
    "command": "editor.action.moveLinesUpAction",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "alt+up",
    "command": "-editor.action.moveLinesUpAction",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+k",
    "command": "editor.action.moveLinesUpAction",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+cmd+up",
    "command": "-editor.action.moveLinesUpAction",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+l",
    "command": "workbench.action.navigateRight"
  },
  {
    "key": "ctrl+h",
    "command": "workbench.action.navigateLeft"
  },
  {
    "key": "alt+w",
    "command": "workbench.action.terminal.kill"
  },
  {
    "key": "cmd+b",
    "command": "-workbench.action.toggleSidebarVisibility"
  },
  {
    "key": "cmd+;",
    "command": "workbench.action.focusFirstEditorGroup"
  },
  {
    "key": "cmd+1",
    "command": "-workbench.action.focusFirstEditorGroup"
  },
  {
    "key": "cmd+0",
    "command": "workbench.action.zoomReset"
  },
  {
    "key": "cmd+numpad0",
    "command": "-workbench.action.zoomReset"
  },
  {
    "key": "ctrl+;",
    "command": "workbench.debug.action.toggleRepl"
  },
  {
    "key": "shift+cmd+y",
    "command": "-workbench.debug.action.toggleRepl"
  },
  {
    "key": "cmd+=",
    "command": "editor.action.fontZoomIn"
  },
  {
    "key": "cmd+-",
    "command": "editor.action.fontZoomOut"
  },
  {
    "key": "cmd+0",
    "command": "editor.action.fontZoomReset"
  },
  {
    "key": "cmd+=",
    "command": "-workbench.action.zoomIn"
  },
  {
    "key": "ctrl+v",
    "command": "workbench.action.newGroupRight"
  },
  {
    "key": "ctrl+s",
    "command": "workbench.action.newGroupBelow"
  },
  {
    "key": "ctrl+o",
    "command": "-extension.vim_ctrl+o",
    "when": "editorTextFocus && vim.active && vim.use<C-o> && !inDebugRepl"
  },
  {
    "key": "cmd+1",
    "command": "workbench.action.editorLayoutSingle"
  },
  {
    "key": "shift+cmd+g",
    "command": "workbench.view.scm",
    "when": "workbench.scm.active && !gitlens:disabled && config.gitlens.keymap == 'chorded'"
  },
  {
    "key": "shift+cmd+g",
    "command": "workbench.view.scm",
    "when": "workbench.scm.active && !gitlens:disabled && config.gitlens.keymap == 'chorded'"
  },
  {
    "key": "ctrl+h",
    "command": "workbench.action.terminal.focusPreviousPane",
    "when": "terminalFocus && terminalProcessSupported"
  },
  {
    "key": "alt+cmd+up",
    "command": "-workbench.action.terminal.focusPreviousPane",
    "when": "terminalFocus && terminalProcessSupported"
  },
  {
    "key": "ctrl+l",
    "command": "workbench.action.terminal.focusNextPane",
    "when": "terminalFocus && terminalProcessSupported"
  },
  {
    "key": "alt+cmd+down",
    "command": "-workbench.action.terminal.focusNextPane",
    "when": "terminalFocus && terminalProcessSupported"
  },
  {
    "key": "alt+cmd+p",
    "command": "-projectManager.listProjects"
  }
]
