{
  "advancedNewFile.convenienceOptions": [
      "current",
      "root"
  ],
  "breadcrumbs.enabled": false,
  "editor.codeActionsOnSave": {
      "source.fixAll": true
  },
  "editor.fontFamily": "\"SF Mono\"",
  "editor.minimap.enabled": false,
  "editor.suggestSelection": "first",
  "eslint.debug": true,
  "editor.tabSize": 2,
  "path-intellisense.mappings": {
      "@": "src"
  },
  "vetur.format.defaultFormatter.html": "prettier",
  "extensions.supportVirtualWorkspaces": {
      "octref.vetur": true
  },     
  "vim.useSystemClipboard": true,
  "workbench.editor.tabSizing": "shrink",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.startupEditor": "newUntitledFile",
  "vim.leader": "<space>",
  "vim.normalModeKeyBindingsNonRecursive": [
      {
          "before": ["<leader>", "o"],
          "commands": ["workbench.action.gotoSymbol"]
      },
      {
          "before": ["<leader>", "p"],
          "commands": ["workbench.action.quickOpen"]
      },
      {
          "before": ["<leader>", "i"],
          "commands": ["workbench.action.quickOpenRecent"]
      },
      {
          "before": ["<leader>", "<leader>"],
          "commands": ["workbench.action.quickOpen"]
      },
      {
          "before": ["<leader>", "w"],
          "commands": ["workbench.action.files.save"]
      },
      {
          "before": ["<leader>", "q"],
          "commands": ["workbench.action.closeWindow"]
      },
      {
          "before": ["<leader>", ";"],
          "commands": ["actions.find"]
      },
      {
          "before": ["<leader>", "/"],
          "commands": ["workbench.action.findInFiles"]
      },
      {
          "before": ["<leader>", "e"],
          "commands": ["workbench.files.action.showActiveFileInExplorer"],
      },
      {
          "before": [",", "w"],
          "commands": ["workbench.action.files.save"]
      },
      {
          "before": [",", "q"],
          "commands": ["workbench.action.closeWindow"]
      },
      {
          "before": [",", "n"],
          "commands": ["workbench.action.toggleSidebarVisibility"]
      },
      {
          "before": ["<space>", "n"],
          "commands": ["workbench.action.toggleSidebarVisibility"]
      },
      {
          "before": ["<space>", "b"],
          "commands": ["workbench.action.toggleSidebarVisibility"]
      },
      {
          "before": ["<space>", "a"],
          "commands": ["workbench.action.toggleActivityBarVisibility"]
      },
      {
          "before": ["<space>", "v"],
          "commands": ["workbench.action.toggleStatusbarVisibility"]
      },
      {
          "before": ["<space>", "d"],
          "commands": ["projectManager.listProjects"]
      },
      {
          "before": ["<space>", "'"],
          "commands": ["workbench.action.terminal.toggleTerminal"]
      },
      {
          "before": ["]", "<space>"],
          "after": ["ma", "o", "<esc>", "k", "'a"]
      },
      {
          "before": ["[", "<space>"],
          "after": ["ma", "O", "<esc>", "j", "'a"]
      },
      {
          "before": ["<leader>", "g"],
          "commands": ["workbench.view.scm"],
      },
  ],
  "workbench.editor.enablePreview": false,
  "[php]": {
      "editor.formatOnSave": true,
      "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "gitlens.statusBar.enabled": false,
  "gitlens.codeLens.enabled": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "editor.autoClosingBrackets": "never",
  "editor.autoClosingQuotes": "never",
  "git.autofetch": true,
  "githubIssues.queries": [
      {
          "label": "All Open",
          "query": "state:open repo:${owner}/${repository} sort:created-desc"
      },
      {
          "label": "My Issues",
          "query": "default"
      },
      {
          "label": "Created Issues",
          "query": "author:${user} state:open repo:${owner}/${repository} sort:created-desc"
      }
  ],
  "android-webview-debug.adbPath": "/Users/seth/Library/Android/sdk/platform-tools/adb",
  "git.enableSmartCommit": true,
  "files.exclude": {
      "**/.classpath": true,
      "**/.project": true,
      "**/.settings": true,
      "**/.factorypath": true
  },
  "[json]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "window.autoDetectColorScheme": true,
  "workbench.preferredDarkColorTheme": "GitHub Dark",
  "workbench.colorTheme": "GitHub Light",
  "workbench.preferredLightColorTheme": "GitHub Light",
  "[javascript]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "explorer.openEditors.visible": 0,
  "security.workspace.trust.untrustedFiles": "newWindow",
  "projectManager.git.baseFolders": [
      "~/Code"
  ],
  "projectManager.openInNewWindowWhenClickingInStatusBar": true,
  "[html]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode",
      "editor.formatOnSave": true
  },
  "[typescript]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode",
      "editor.formatOnSave": true
  },
  "files.associations": {
      "*.env.development": "env",
      "*.env.production": "env"
  },
  "php.validate.executablePath": "/usr/local/bin/php",
  "terminal.integrated.profiles.osx": {
      "bash": {
          "path": "/usr/local/bin/bash",
          "args": [
              "-l"
          ],
          "icon": "terminal-bash"
      },
      "zsh": {
          "path": "/usr/local/bin/zsh",
          "args": [
              "-l"
          ]
      },
      "fish": {
          "path": "/usr/local/bin/fish",
          "args": [
              "-l"
          ],
      },
      "tmux": {
          "path": "/usr/local/bin/tmux",
          "icon": "terminal-tmux",
      },
      "tmuxplus": {
          "path": "/usr/local/bin/vscode-shell",
          "icon": "terminal-tmux",
      },
  },
  "terminal.integrated.defaultProfile.osx": "fish",
  "search.quickOpen.includeHistory": false,
  "explorer.confirmDelete": false,
  "[typescriptreact]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.fontSize": 14,
  "i18n-ally.enabledParsers": [
      "json"
  ],
  "workbench.statusBar.visible": false,
  "[vue]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.activityBar.visible": false,
}